{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "jest": true
    },
    "extends": [
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
      "require-jsdoc": "off",
      "max-len": [
        "error",
        {
          "code": 100,
          "tabWidth": 2,
          "ignorePattern": "require"
        }
      ]
    }
}
